{
  "source": {
    "include": ["./src/"],
        "includePattern": ".+\\.(js|ts)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "templates": {
    "better-docs": {
      "name": "EVE Companion App",
      "logo": "",
      "hideGenerator": true
    }
  }
}
