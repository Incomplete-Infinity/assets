{
  "source": {
    "include": ["./src/", "./src/api/esi"],
    "includePattern": ".+\\.(js|ts)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "node_modules/better-docs/typescript"],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "templates": {
    "better-docs": {
      "name": "EVE Companion App",
      "logo": "",
      "hideGenerator": true
    }
  }
}
